@using RPS.Web.Server.Components.Shared


<div style="width: 1440px; height: 54px; padding-left: 16px; padding-right: 16px; padding-top: 8px; padding-bottom: 8px; background: rgba(3.84, 86.84, 98.16, 0.06); box-shadow: 0px 6px 13px rgba(0, 0, 0, 0.12); justify-content: space-between; align-items: center; display: inline-flex">
  <div style="justify-content: flex-start; align-items: center; gap: 72px; display: flex">


    <div style="justify-content: flex-start; align-items: center; gap: 2px; display: flex">
      <Logo />


    </div>
    <div style="justify-content: flex-start; align-items: flex-start; gap: 4px; display: flex">
      <div style="padding-left: 12px; padding-right: 12px; padding-top: 6px; padding-bottom: 6px; border-radius: 8px; border: 1px rgba(255, 255, 255, 0) solid; justify-content: center; align-items: center; gap: 8px; display: flex">
        <div style="text-align: center; color: #068597; font-size: 16px; font-family: Roboto; font-weight: 500; line-height: 24px; word-wrap: break-word">Teams</div>
      </div>
      <div style="padding-left: 12px; padding-right: 12px; padding-top: 6px; padding-bottom: 6px; border-radius: 8px; border: 1px rgba(255, 255, 255, 0) solid; justify-content: center; align-items: center; gap: 8px; display: flex">
        <div style="text-align: center; color: #068597; font-size: 16px; font-family: Roboto; font-weight: 500; line-height: 24px; word-wrap: break-word">Plans</div>
      </div>
      <div style="padding-left: 12px; padding-right: 12px; padding-top: 6px; padding-bottom: 6px; border-radius: 8px; border: 1px rgba(255, 255, 255, 0) solid; justify-content: center; align-items: center; gap: 8px; display: flex">
        <div style="text-align: center; color: #068597; font-size: 16px; font-family: Roboto; font-weight: 500; line-height: 24px; word-wrap: break-word">Apps</div>
      </div>
    </div>
  </div>
  <div style="justify-content: flex-end; align-items: center; gap: 8px; display: flex">
    <div style="justify-content: center; align-items: center; gap: 8px; display: flex">
      <div style="width: 30px; height: 30px; border-radius: 9999px; justify-content: flex-start; align-items: flex-start; display: flex">
        <div style="flex: 1 1 0; align-self: stretch; border-radius: 9999px; overflow: hidden; flex-direction: column; justify-content: center; align-items: center; display: inline-flex">
          <img style="width: 30px; height: 30px; position: relative" src="https://via.placeholder.com/30x30" />
        </div>
      </div>
    </div>
  </div>
</div>